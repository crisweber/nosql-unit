<fo:inline font-weight="bold">public</fo:inline> <fo:inline font-weight="bold">class</fo:inline> WhenNeoMeetsANewFriend {

	<fo:inline color="gray">@ClassRule</fo:inline>
	<fo:inline font-weight="bold">public</fo:inline> <fo:inline font-weight="bold">static</fo:inline> ManagedWrappingNeoServer managedWrappingNeoServer = newWrappingNeoServerNeo4jRule().build();
	
	<fo:inline color="gray">@Rule</fo:inline>
	<fo:inline font-weight="bold">public</fo:inline> Neo4jRule neo4jRule = <fo:inline font-weight="bold">new</fo:inline> Neo4jRule(newManagedNeoServerConfiguration().build(), <fo:inline font-weight="bold">this</fo:inline>);
	
	<fo:inline color="gray">@Inject</fo:inline>
